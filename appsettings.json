{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db;Cache=Shared",
    "PostgresqlConnection": "Host={port};Port={port};Database={database};Username={user};Password={password}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtConfig": {
    "Secret": "YourSecret",
    "ExpiryTimeFrame": "00:01:00"
  },
  "FileStorage": {
    "TypeFolders": {
      "4": "RECEPCION_ENTREGA",
      "5": "RECEPCION_ENTREGA",
      "6": "MEMOS",
      "7": "MEMOS",
      "8": "COTIZACION"
    }
    },
  "DB_Archivotypes": {
    "ENTREGA_RECEPCION_GENERADA": 4,
    "ENTREGA_RECEPCION": 5,
    "MEMO_GENERADA": 6,
    "MEMO_DIGITALIZADO": 7,
    "COTIZACION_DIGITALIZADA" : 8
  },
  "DB_ESTADOS": {
    "ELIMINADO": 3,
    "PENDIENTE_REVISION_COTIZACION":  4
  },
    "PATH_MASTER": "wwwroot/Formats/"
  }
