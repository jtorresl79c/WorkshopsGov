{
  "private": true,
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ]
  },
  "scripts": {
    "prestart": "dotnet build",
    "start": "dotnet run",
    "build": "cd ClientApp && pnpm build",
    "dev": "concurrently \"dotnet run\" \"cd ClientApp && pnpm dev\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "animejs": "^3.2.2",
    "chart.js": "^4.4.8",
    "vue-chartjs": "^5.3.2"
  }
}